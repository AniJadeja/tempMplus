    "homepage": "https://gitlab.com/nareshbarfa007/mplus-admin-server/-/blob/main/README.md",
    "dependencies": {
        "ava": "^6.0.0",
        "bcrypt": "^5.1.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "module-alias": "^2.2.3",
